{{ form_start(form) }}
    <label>Titre</label>
    {{ form_widget(form.title,{'attr':{'class':'form form-control'}}) }}<br><br>
    <label>Description</label>
    {{ form_widget(form.description,{'attr':{'class':'form form-control'}}) }}<br><br>
    <label>Categorie</label>
    {{ form_widget(form.category,{'attr':{'class':'form form-control'}}) }}<br><br>
    <label>Ajourter une Image</label>
    {{ form_widget(form.imageFile,{'attr':{'class':'form form-control'}}) }}<br><br>
    <button class="btn btn-primary btn-block">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
